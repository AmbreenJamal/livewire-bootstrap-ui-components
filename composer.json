{
    "name": "ambreenjamal/livewire-bootstrap-ui-components",
    "description": "laravel blade components with bootstrap fornt end",
    "keywords": [
        "AmbreenJamal",
        "laravel",
        "livewire-bootstrap-ui-components"
    ],
    "homepage": "https://github.com/ambreenjamal/livewire-bootstrap-ui-components",
    "license": "MIT",
    "authors": [
        {
            "name": "Ambreen Jamal",
            "email": "ambreen.jamal@hotmail.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Ambreenjamal\\BootstrapUiComponents\\": "src",
            "Ambreenjamal\\BootstrapUiComponents\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ambreenjamal\\BootstrapUiComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ambreenjamal\\BootstrapUiComponents\\BootstrapUiComponentsServiceProvider"
            ],
            "aliases": {
                "BootstrapUiComponents": "Ambreenjamal\\BootstrapUiComponents\\BootstrapUiComponentsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
